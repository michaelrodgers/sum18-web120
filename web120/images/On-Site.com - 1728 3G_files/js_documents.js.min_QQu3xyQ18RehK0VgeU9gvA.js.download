function showAddenda(b){var c=document.getElementById("addendalink"),a=true;if(c.show==true){a=false;c.show=false}else{a=true;c.show=true}if(a){c.innerHTML=c.innerHTML.replace("Show","Hide");if(document.getElementById("addendaprint")){document.getElementById("addendaprint").style.visibility=""}}else{c.innerHTML=c.innerHTML.replace("Hide","Show");if(document.getElementById("addendaprint")){document.getElementById("addendaprint").style.visibility="hidden"}}for(i=0;i<b;i++){var c=document.getElementById("addendum"+i);if(a){c.style.display=""}else{c.style.display="none"}}};
